
config wifi-device 'radio0'
	option type 'mac80211'
	option channel '11'
	option hwmode '11g'
	option path 'platform/ar933x_wmac'
	option htmode 'HT20'
	option noscan '1'

config wifi-iface
	option device 'radio0'
	option network 'lan'
	option mode 'ap'
	option ssid 'ROOter'
	option encryption 'psk'
	option key 'rooter2017'


config wifi-iface
	option network 'wwan_1'
	option ssid 'ZTE_2.4G_pcT6ej'
	option encryption 'psk2'
	option device 'radio0'
	option mode 'sta'
	option bssid '24:D3:F2:F7:86:FC'
	option key 'likalikulakilaki'

